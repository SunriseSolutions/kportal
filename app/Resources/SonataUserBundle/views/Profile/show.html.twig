    <script>
        $(function () {
            window.skills = [];

            {% for skill in skillDb %}

            {% if skill not in skills %}
            window.skills.push({value: '{{ skill.name }}', id: '{{ skill.id }}'});
            {% endif %}

            {% endfor %}
            window.drdownSrch('#skill-srch', window.skills, function (selected) {

            });


            var _date = new Date();
            var _day = _date.getDay();
            var _month = _date.getMonth();
            var _year = _date.getFullYear() - 16;

            $('.birthday-input').datepicker({
                format: "dd/mm/yyyy",
                language: "{{ app.request.locale }}",
                autoclose: true,
                toggleActive: true,
                defaultViewDate: {year: _year, month: _month, day: _day}
            });
        });

        (function ($) {
            /* OnLoad Page */
            $(document).ready(function ($) {

            });
            /* OnLoad Window */
            var init = function () {
                $('body').removeClass('page-loader-2');
                $('body').removeClass('pace-done');
            };
            window.onload = init;

        })(jQuery);


    </script>

