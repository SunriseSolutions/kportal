{% extends "::base.html.twig" %}
{% block main %}
    {% set profile = app.user.profile %}
    {% set skills = profile.skills %}
    {% set candidates = app.user.candidates %}
    {#{% include "SonataUserBundle:Profile:action.html.twig" %}#}
    <section class="main main-profile container">
        {% if profile.getMissingFields|length > 0 %}
            <div class="removable warning"><i class="fa fa-info-circle"></i><span><strong>{{ app.user.firstName }},</strong>{{ 'profile.title_add_more_details'|trans({},'SonataUserBundle') }}</span><a
                    href="javascript:;" class="warning-msg pull-right"><i class="fa fa-remove click-to-remove"></i></a>
            </div>
        {% endif %}
        <div class="row">
            <div class="col-xs-12 col-sm-8 col-md-9">
                <div class="section-detail section-resume-header">
                    <div class="box">
                        <div class="user-info border-primary">
                            <div class="row">
                                <div class="col-xs-4 col-sm-3 center-block">
                                    <div class="user-pic">
                                        <a href="#" data-toggle="modal" data-target="#upload-img"
                                           class="avatar img-circle">
                                            <img id="avatar-img"
                                                 src="{{ userAvatar(app.user,'admin') }}"/>
                                            <p><i class="fa fa-camera"></i><span> {{ 'profile.change_avatar'|trans({},'SonataUserBundle') }}</span></p>
                                        </a>
                                    </div>
                                </div>
                                <div class="section-header hidden-section col-xs-8 col-sm-9">
                                    <form method="post" id="general-form" class="form-wrapper" name="general"
                                          action="{{ path('post_profile_general',{'user':app.user.id}) }}">
                                        <div class="relative box-md"><span class="form-btn-edit">
                            <button type="button" class="btn"><i class="fa fa-pencil"></i></button></span>
                                            <div class="profile-info">
                                                <div class="prof-itm profile-name clearfix">
                                                    <div class="name pull-left">
                                                        <div class="view-field">
                                                            <h3 class="fname profile-name text-orange">{{ app.user.firstName }}</h3>
                                                        </div>
                                                        <div class="edit-field form-group">
                                                            <label><strong class="text-red">*</strong> {{ 'profile.label_last_name'|trans({},'SonataUserBundle') }}</label>
                                                            <input type="text" name="lastName" autofocus=""
                                                                   value="{{ app.user.lastName }}" class="form-control"
                                                                   required="required">
                                                        </div>
                                                    </div>
                                                    <div class="last-name pull-left">
                                                        <div class="view-field">
                                                            <h3 class="lname profile-name text-orange">{{ app.user.lastName }}</h3>
                                                        </div>
                                                        <div class="edit-field">
                                                            <label><strong class="text-red">*</strong> {{ 'profile.label_first_name'|trans({},'SonataUserBundle') }} </label>
                                                            <input type="text" name="firstName" autofocus=""
                                                                   value="{{ app.user.firstName }}" class="form-control"
                                                                   required="required">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="prof-itm prof-itm-sm clearfix">
                                                    <div class="view-field"><span
                                                                class="general_title view-control ">{{ profile.title|default('') }}</span>
                                                    </div>
                                                    <div class="edit-field form-group">
                                                        <label><strong class="text-red">*</strong>{{ 'profile.label_professional_title'|trans({},'SonataUserBundle') }}</label>
                                                        <input type="text" name="professionalTitle"
                                                               value="{{ profile.title|default('') }}"
                                                               class="form-control" required="required">
                                                    </div>
                                                </div>
                                                <div class="prof-itm prof-itm-sm clearfix">

                                                    <div class="view-field"><span class="year-of-ex-view-field"><span
                                                                    class="general_years view-control">{{ profile.yearsOfExperience }}
                                                                {{ 'profile.years_of_experience'|trans({},'SonataUserBundle') }}</span> </span></div>

                                                    <div class="edit-field form-group">
                                                        <label class="lbl-year-ex"><strong class="text-red">*</strong>
                                                            {{ 'profile.label_years_of_experience'|trans({},'SonataUserBundle') }}</label>
                                                        <input type="text" name="yearsOfExperience" maxlength="2"
                                                               value="{{ profile.yearsOfExperience|default('') }}"
                                                               class="form-control" required="required">
                                                    </div>
                                                </div>
                                                {#
                                              <div class="prof-itm prof-itm-sm clearfix">
                                                  <div class="view-field">
                                                  </div>

                                                  <div class="edit-field form-group m-t-n-xs">
                                                      <label>
                                                          <input type="checkbox" name="newGraduate" value="false">   I am a new graduate / have no work experience
                                                      </label>
                                                  </div>
                                              </div>
                                              #}

                                                <div class="prof-itm prof-itm-sm clearfix">
                                                    {#
                                                    <div class="view-field hidden-xs">
                                                        <p><span class="text-gray-light">Latest company:</span><a class="current-company">Staff.com</a><br><span class="text-gray-light">Highest education:</span><a class="current-highest-education">Bachelors</a></p>
                                                    </div>
                                                    #}
                                                    <div class="edit-field text-right">
                                                        <button type="button" class="btn-cancel btn">Cancel</button>
                                                        <button type="submit" class="btn-save btn btn-primary">Save
                                                        </button>
                                                    </div>
                                                </div>

                                                {#
                                                <div class="prof-itm prof-itm-sm clearfix download-resume">
                                                    <div class="view-field"><a href="javascript:;" class="btn btn-outline"><i class="fa fa-fw fa-file-pdf-o"></i> Download profile</a></div>
                                                </div>
                                                #}
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div> <!-- ME End: PROFILE INFO-->

                            <div class="bg-gray text-right">
                                <div class="clearfix"><a href="javascript:;" data-target="#my-job-tab"
                                                         class="btn tab-prof add-one-more-section col-xs-12 col-md-4 active"><i
                                                class="fa fa-fw fa-suitcase"></i><strong>{{ 'profile.title_my_jobs'|trans({},'SonataUserBundle') }}</strong></a><a
                                            href="javascript:;" data-target="#media-tab"
                                            class="btn tab-prof add-one-more-section col-xs-12 col-md-4"><i
                                                class="fa fa-fw fa-cubes"></i><strong>{{ 'profile.title_my_resume_cv'|trans({},'SonataUserBundle') }}</strong></a><a
                                            id="profile-tab" href="javascript:;" data-target="#professional-profile-tab"
                                            class="btn tab-prof add-one-more-section col-xs-12 col-md-4"><i
                                                class="fa fa-fw fa-phone"></i><strong>{{ 'profile.title_professional_profile'|trans({},'SonataUserBundle') }}</strong></a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-prof-wrap" style="display: block;">
                        <div id="professional-profile-tab" style="display: none;" class="section-contact tab-prof-ct">
                            <div class="section-detail section-contact-info">
                              <div class="box">
                                <form class="form-horizontal" action="{{ path('post_profile_contact_info') }}" method="post"
                                  id="contact-info-form">
                                <div class="box">
                                    <fieldset>
                                        <div class="form-wrapper relative box-md"><span class="form-btn-edit">
                          <button type="button" class="btn btn-sm"><i class="fa fa-pencil"></i></button></span>
                                            <div class="row">
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label for="address">{{ 'form.email'|trans({},'SonataUserBundle') }}</label>
                                                        <div class="view-field">
                                                            <p class="form-control-static view-control">
                                                                {{ app.user.email }}</p>
                                                        </div>
                                                        <div class="edit-field">
                                                            <input type="email" autofocus="" disabled=""
                                                                   value="{{ app.user.email }}" class="form-control">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label for="cell-number"><strong class="text-red">*</strong>{{ 'profile.label_mobile_number'|trans({},'SonataUserBundle') }} </label>
                                                        <div class="view-field clearfix">
                                                            <p class="phone form-control-static view-control pull-left">
                                                                {{ app.user.phone }}</p>
                                                            {#
                                                            <div class="pull-left form-control-static text-gray-light">
                                                                <em><i class="fa fa-eye"></i><span>  Visible to employer</span></em>
                                                            </div>
                                                            #}

                                                        </div>
                                                        <div class="edit-field">
                                                            {{ form_widget(contactForm.phone) }}

                                                            {#
                                                            <label class="push-top-sm">
                                                                <input name="confidential" value="1" type="checkbox"><em>
                                                                    Invisible to employer</em>
                                                            </label>
                                                            #}
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label for="date-of-birth"><strong class="text-red"></strong>{{ 'form.label_date_of_birth'|trans({},'SonataUserBundle') }}</label>
                                                        <div class="edit-field">

                                                            {{ form_widget(contactForm.birthday) }}

                                                        </div>
                                                        <div class="view-field">
                                                            <p class="birthday form-control-static view-control">
                                                                {% if app.user.dateOfBirth is not empty %}
                                                                {{ app.user.dateOfBirth|date('d/m/Y') }}
                                                                {% endif %}
                                                            </p>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label for="nationality"><strong class="text-red">*</strong>{{ 'profile.label_nationality'|trans({},'SonataUserBundle') }}</label>
                                                        <div class="view-field">
                                                            <p class="nationality form-control-static view-control">{{ app.user.nationality }}</p>
                                                        </div>
                                                        <div class="edit-field">
                                                            {{ form_widget(contactForm.nationality) }}
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label><strong class="text-red">*</strong> {{ 'form.label_gender'|trans({},'SonataUserBundle') }}</label>
                                                        {{ form_widget(contactForm.gender,{'attr':
                                                            {'class':'edit-field'}
                                                        }) }}

                                                        <div class="view-field">
                                                            <p class="gender form-control-static view-control">
                                                                {% if app.user.gender is not empty %}
                                                                    {% if app.user.gender != 'u' %}
                                                                    {{ app.user.gender == 'm'? 'gender_male'|trans({},'SonataUserBundle')|capitalize:'gender_female'|trans({},'SonataUserBundle')|capitalize }}
                                                                    {% endif %}
                                                                {% endif %}
                                                            </p>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label> {{ 'profile.label_marital_status'|trans({},'SonataUserBundle') }}</label>
                                                        {{ form_widget(contactForm.marital_status,{'attr':
                                                            {'class':'edit-field'}
                                                        }) }}

                                                        <div class="view-field">
                                                            <p class="marital-status form-control-static view-control">
                                                                {% if app.user.maritalStatus is not empty %}
                                                                    {{ app.user.maritalStatus == 'm'?'form.marital_married'|trans({}):'form.marital_single'|trans({}) }}
                                                                {% endif %}
                                                            </p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <input type="hidden" id="place-id" name="street_number"/>
                                            <input type="hidden" id="street_number" name="street_number"/>
                                            <input type="hidden" id="route" name="route"/>
                                            <input type="hidden" id="locality" name="locality"/>
                                            <input type="hidden" id="administrative_area_level_1"
                                                   name="administrative_area_level_1"/>
                                            <input type="hidden" id="postal_code" name="postal_code"/>
                                            <input type="hidden" id="country" name="country"/>

                                            <div class="form-group no-margin">
                                                <label for="address"><strong class="text-red"></strong> Address</label>
                                                <div class="view-field">
                                                    <p class="address form-control-static view-control">{{ app.user.address.address }}</p>
                                                </div>
                                                <div class="edit-field">
                                                    {{ form_row(contactForm.address) }}
                                                </div>
                                            </div>
                                            <!-- Buttons-->
                                            <div class="form-group edit-field edit-btns">
                                                <div class="pull-left">(<strong class="text-red">*</strong>) {{ 'form.required_field'|trans() }}
                                                </div>
                                                <div class="pull-right text-right">
                                                    <button type="button" class="btn btn-cancel">{{ 'modal.button_cancel'|trans({},'AppBundle') }}</button>
                                                    <button type="submit" class="btn btn-primary btn-save">{{ 'modal.button_save'|trans({},'AppBundle') }}</button>
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                </div>
                            </form>
                              </div>
                            </div>

                            <!-- Profile Summary -->
                            <div class="section-detail section-profile">
                                <div class="box">
                                    <fieldset>
                                        <legend class="box-md"><i class="fa fa-fw fa-list"></i> {{ 'profile.title_summary'|trans({},'SonataUserBundle') }}</legend>
                                        <div class="relative">
                                            <form id="summary-form" name="summary" class="form-horizontal box-md form-wrapper"
                                                  action="{{ path('post_profile_summary',{'user':app.user.id}) }}"
                                                  method="post"><span class="form-btn-edit">
                        <button type="button" class="btn btn-sm"><i class="fa fa-pencil"></i></button></span>
                                                <div class="form-group m-b-none">
                                                    <div class="view-field summary">
                                                        {{ profile.summary|default('<p>Introduce yourself and describe your career objectives</p>')|raw }}
                                                    </div>
                                                    <div class="edit-field">
                                                        {#
                                                           <p>
                                                               <label>Introduce yourself and describe your career objectives</label>
                                                           </p>
                                                         #}
                                                        {{ form_row(summaryForm.summary) }}
                                                        {#
                                                        <textarea rows="5" autofocus="" name="summary" maxlength="5000" class="form-control countdown-target"></textarea>
                                                        #}

                                                        <em class="countdown gray-light">
                                                            {#5000 characters remaining#}
                                                        </em>
                                                    </div>
                                                </div>
                                                <!-- Buttons-->
                                                <div class="form-group push-top-md edit-field edit-btns clearfix">
                                                    <div class="text-right">
                                                        <button type="button" class="btn btn-cancel">{{ 'modal.button_cancel'|trans({},'AppBundle') }}</button>
                                                        <button type="submit" class="btn btn-save btn-primary">{{ 'modal.button_save'|trans({},'AppBundle') }}</button>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </fieldset>
                                </div>
                            </div>
                            <!-- End Profile Summary -->
                            <!-- Skills -->
                            <div class="section-detail section-skills skill-section">
                                <div class="box">
                                    <fieldset>
                                        <legend class="box-md"><i class="fa fa-fw fa-flash"></i> {{ 'profile.title_key_skills'|trans({},'SonataUserBundle') }}</legend>
                                        <div class="form-wrapper box-md skill-section relative clearfix">
                                <span
                                        class="form-btn-edit">
                      <button type="button" class="btn btn-sm"><i class="fa fa-pencil"></i></button>
                                </span>
                                            <div id="view-skills" class="view-skills view-field">
                                                {% for skill in skills %}
                                                    <span class="tag-xs">
                                            <span class="tag-name">{{ skill.name }}</span>
                                        </span>
                                                {% endfor %}

                                            </div>
                                            <div hidden="" class="edit-skills edit-field">
                                                <form id="skill-form" method="post" action="{{ path('post_profile_skills') }}"
                                                      name="skill" class="form-horizontal">
                                                    <div class="content-section relative">
                                                        <div id="skill-list-field" class="tags-edit">
                                                            {% for skill in skills %}
                                                                <span class="tag-xs">
                                                        <span class="tag-name">{{ skill.name }}</span>
                                                        <a href="javascript:;"><i
                                                                    class="fa fa-fw fa-remove"></i></a>
                                                        <input type="hidden" value="{{ skill.name }}" name="skills[]"/>
                                                    </span>
                                                            {% endfor %}

                                                        </div>
                                                        <div class="skill-input">
                                                            <div class="auto-dropdown">
                                                                <input id="skill-srch" type="text" name="skills[]" autofocus
                                                                       class="form-control">
                                                            </div>
                                                            <button type="button" class="btn"><i class="fa fa-plus"></i></button>
                                                        </div>
                                                    </div>
                                                    <div class="push-top-sm pull-right">
                                                        <button type="button" class="btn btn-cancel">Cancel</button>
                                                        <button type="submit" class="btn btn-primary">Save</button>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </fieldset>
                                </div>
                            </div>
                            <!-- End Skills -->



                        </div>
                        <div id="media-tab" style="display: none;" class="tab-prof-ct">
                            <div class="box">
                                <fieldset>
                                    <div class="content-section">
                                        <div class="form-wrapper box-md">
                                            <div class="add-media well well-sm dash">

                                                <div class="row">
                                                    {#<div class="media-upl t-c col-md-5"> Rooms for YouTube#}
                                                    <div class="media-upl t-c col-md-12">
                                                        <p class="media-desc">{{ 'profile.allowed_types_size'|trans({'max_file_size':max_file_size,'max_file_per_user':max_file_per_user},'SonataUserBundle') }}</p>
                                                        <form method="post" id="upload-cv" enctype="multipart/form-data"
                                                              action="{{ path('post__file') }}"><span
                                                                    id="file-name-info"></span>
                                                            <input accept=".doc,.docx,.pdf" type="file"
                                                                   name="binaryContent" id="media-upl" class="hidden"/>
                                                            <label for="media-upl"
                                                                   class="btn btn-default">{{ 'job.application.upload_cv_button'|trans({},'AppBundle') }}</label>
                                                        </form>
                                                        {#
                                                        <span
                                                                class="text-or"><span class="line"></span><span
                                                                    class="or-ct">OR</span></span>
                                                        #}
                                                    </div>
                                                    {#
                                                    <form id="upload-youtube" method="post"
                                                          action="{{ path('post__youtube') }}">
                                                        <div class="media-youtb t-c col-md-5">
                                                            <p class="media-desc">You can copy & paste to embed up to 7
                                                                youtube videos. <strong>Eg:</strong> <code>https://youtu.be/RtbcZmCc7rg</code>
                                                                or
                                                                <code>https://www.youtube.com/watch?v=RtbcZmCc7rg</code> </code>
                                                            </p>

                                                            <input type="text" name="binaryContent"
                                                                   placeholder="YouTube embed code"
                                                                   class="form-control">
                                                        </div>
                                                        <div class="media-btn-group t-c col-md-2">
                                                            <button href="javascript:;"
                                                                    class="btn btn-primary">ADD
                                                            </button>
                                                        </div>
                                                    </form>
                                                    #}
                                                </div>

                                            </div>
                                            <!-- End: UPLOAD BLK-->
                                            <div class="table-responsive">
                                                <table class="table" id="media-tbl">
                                                    <tr class="tbl-head">
                                                        <th></th>
                                                        <th>{{ 'profile.media.title_name_link'|trans({},'SonataUserBundle') }}</th>
                                                        <th class="t-c">{{ 'profile.media.title_size'|trans({},'SonataUserBundle') }}</th>
                                                        <th colspan="2" class="t-c">{{ 'profile.media.title_action'|trans({},'SonataUserBundle') }}</th>
                                                    </tr>
                                                    {% for medium in app.user.media %}
                                                        <tr class="row-entity">
                                                            <td class="fa {{ medium|mediaExt|faMediaIcon }}"></td>
                                                            <td>
                                                                <span data-id="{{ medium.id }}"
                                                                      data-placeholder="{{ medium.name }}"
                                                                      class="medium-name edittable"
                                                                      style="display: inline;"><span
                                                                            class="edit-ct">{{ medium.name }}</span></span>
                                                            </td>
                                                            <td class="t-c">{{ medium.size|formatBytes }}</td>

                                                            <td class="t-c" colspan="2">
                                                                <a target="_blank"
                                                                   href="{{ path('sonata_media_download',{'id':medium.id}) }}"
                                                                   class="del-row">
                                                                    <i class="fa fa-download"></i>
                                                                </a>
                                                                &nbsp;
                                                                <a href="javascript:;" data-id="{{ medium.id }}"
                                                                   data-entity="media" class="del-row del-action">
                                                                    <i class="fa fa-trash-o"></i>
                                                                </a>
                                                            </td>
                                                        </tr>
                                                    {% endfor %}
                                                </table>
                                            </div>
                                            <!-- End: MEDIA TABLE-->
                                        </div>
                                    </div>
                                </fieldset>
                            </div>
                            <!-- End: TAB MEDIA-->
                        </div>
                        <div id="my-job-tab" style="display: block;" class="tab-prof-ct">
                            <div class="box">
                                <fieldset>
                                    <div class="content-section">
                                        <div class="form-wrapper box-md">
                                            <p><!-- Don't know what to put here --></p>
                                            <div class="table-responsive">
                                                <table class="table table-striped">
                                                    <tr class="tbl-head">
                                                        <th class="t-c">{{ 'profile.my_job.title_title'|trans({},'SonataUserBundle') }}</th>
                                                        <th class="t-c">{{ 'profile.my_job.title_status'|trans({},'SonataUserBundle') }}</th>
                                                        <th class="t-c">{{ 'profile.my_job.title_resume_cv'|trans({},'SonataUserBundle') }}</th>
                                                        <th class="t-c">{{ 'profile.my_job.title_intro_videos'|trans({},'SonataUserBundle') }}</th>
                                                        <th class="t-c">{{ 'profile.my_job.title_video_interview'|trans({},'SonataUserBundle') }}</th>
                                                    </tr>
                                                    {% if candidates|length == 0 %}
                                                        {{ 'profile.my_job.no_active_application'|trans({},'SonataUserBundle') }}
                                                    {% endif %}
                                                    {% for candidate in candidates %}
                                                        <tr>
                                                            <td class="t-c">
                                                                <strong>
                                                                <a target="_blank"
                                                                   href="{{ path('app_job_detail',{'organisation':candidate.listing.organisation.slug,'listing':candidate.listing.slug}) }}">{{ candidate.listing.title }}</a>
                                                                </strong>
                                                            </td>
                                                            <td class="t-c txt-{{ candidate.status|candidateStatusColour }}">
                                                                <a id="status-{{ candidate.id }}" href="#"
                                                                   class="">{{ candidate.status|lower|capitalize }}</a>
                                                                <script>
                                                                    (function ($) {
                                                                        $('#status-{{ candidate.id }}').on('click', function (e) {
                                                                            e.preventDefault();
                                                                            var _html = '<h3>' + '{{ (('candidate.status.' ~ candidate.status)|lower) |trans({},'AppBundle') }}' + '</h3>';
//                                                                        _html += '<ul>';
                                                                            {% if candidate.listing.interviewRequired and candidate.interviewed == false %}
//                                                                        _html += '<li>';
                                                                            _html += '<h3> &nbsp; - ';
                                                                            _html += '{{ 'candidate.status.need_interview'|trans({},'AppBundle') }}';
                                                                            _html += '</h3>';
//                                                                        _html += '</li>';
                                                                            {% endif %}

                                                                            {% if candidate.resume is empty %}
//                                                                        _html += '<li>';
                                                                            _html += '<h3> &nbsp; - ';
                                                                            _html += '{{ 'candidate.status.need_resume'|trans({},'AppBundle') }}';
                                                                            _html += '</h3>';
//                                                                        _html += '</li>';
                                                                            {% endif %}

                                                                            {% if candidate.listing.introductionRequired and (candidate.introVideos|length == 0) %}
//                                                                        _html += '<li>';
                                                                            _html += '<h3> &nbsp; - ';
